<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="DarkerGrayColor">#151515</Color>
    <Color x:Key="DarkGrayColor" >#252525</Color>
    <Color x:Key="MidGrayColor" >#2d2d2d</Color>
    <Color x:Key="LightGrayColor" >#737373</Color>

    <SolidColorBrush x:Key="DarkerGray" Color="{DynamicResource DarkerGrayColor}"/>
    <SolidColorBrush x:Key="DarkGray" Color="{DynamicResource DarkGrayColor}"/>
    <SolidColorBrush x:Key="MidGray" Color="{DynamicResource MidGrayColor}"/>
    <SolidColorBrush x:Key="LightGray" Color="{DynamicResource LightGrayColor}"/>

    <SolidColorBrush x:Key="ButtonBarHover" Color="#00D8F5"/>

    <Thickness x:Key="TextOffset" Left="-2.5" Top="-2.5" Right="-2.5" Bottom="0"/>
    <Thickness x:Key="NumericTextOffset" Left="-2.5" Top="-4.4" Right="-2.5" Bottom="-2.9"/>
</ResourceDictionary>